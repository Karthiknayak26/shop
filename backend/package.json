{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "prod": "NODE_ENV=production node server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "echo \"Backend build completed\"",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "seed": "node scripts/seed.js",
    "seed:clear": "node scripts/seed.js --clear",
    "migrate": "node scripts/migrate.js",
    "backup:create": "node scripts/backup.js create",
    "backup:restore": "node scripts/backup.js restore",
    "backup:list": "node scripts/backup.js list",
    "backup:clean": "node scripts/backup.js clean",
    "db:init": "npm run migrate && npm run seed",
    "db:reset": "npm run seed:clear",
    "logs": "tail -f logs/app.log",
    "health": "curl http://localhost:5000/health || echo 'Server not running'",
    "test:payments": "node scripts/testPayments.js",
    "test:payments:prod": "NODE_ENV=production node scripts/testPayments.js",
    "test:payments:url": "node scripts/testPayments.js --url"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "description": "",
