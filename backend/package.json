    "axios": "^1.7.9",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.17.0",
    "mongoose": "^8.15.2",
    "nodemailer": "^7.0.5",
    "qrcode": "^1.5.4",
    "rate-limit-redis": "^4.2.0",
    "razorpay": "^2.9.6",
    "redis": "^4.6.10",
    "sanitize-html": "^2.11.0",
    "stripe": "^18.3.0",
    "validator": "^13.15.15",
    "xss": "^1.0.14"
  }
}
